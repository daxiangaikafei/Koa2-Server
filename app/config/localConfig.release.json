{
    "error": {
        "200": "未登录",
        "404": "未发现",
        "500": "内部错误",
        "1000": "超时",
        "405": "不允许使用该方法"
    },
    "routes": {
        "qbii": {
            "domain": "http://nodeqbii.qbao.com",
            "prefix": "/api/qbii",
            "timeout": 5000,
            "routes": {
                "/user/userId": {
                    "url": "/api/user/${userId}/userId",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/level": { 
                    "url": "/api/user/${userId}/level",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/userInfo": {
                    "url": "/api/user/${userId}/userInfo",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/news/getNewsList": {
                    "url": "/api/news/getNewsList.html",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/news/getNewsDetail/": {
                    "url": "/api/news/getNewsDetail",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/userOrderList": {
                    "url": "/api/user/${userId}/userOrderList.html",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/userProfitList": {
                    "url": "/api/user/${userId}/userProfitList.html",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/project/list": {
                    "url": "/api/project/${userId}/list",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/page/:projectId": {
                    "url": "/api/page/${userId}/${projectId}",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/news/isFinishNews.html": {
                    "url": "/api/news/${userId}/isFinishNews.html",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                }
            }
        },
        "good": {
            "domain": "http://192.168.7.104",
            "prefix": "/api/good",
            "timeout": 5000
        },
        "sys": {
            "domain": "http://192.168.131.145:10550", 
            "prefix": "/api",
            "timeout": 5000
        },
        "item": {
            "domain": "http://192.168.132.44:8081/goods",
            "prefix": "/api/item",
            "timeout": 5000,
            "routes": {
                "/config/wqTokenConfig": {
                    "url": "/config/wqTokenConfig",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/goodsCoupon/couponList": {
                    "url": "/goodsCoupon/${userId}/couponList",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/goodsCoupon/couponDetail/:goodsId": {
                    "url": "/goodsCoupon/${userId}/${goodsId}/couponDetail",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/goodsCoupon/catogry/all": {
                    "url": "/goodsCoupon/catogry/all",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/getUserPoints": {
                    "url": "/user/getUserPoints/${userId}",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/order/list": {
                    "url": "/user/order/list/${userId}",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                },
                "/user/order/submitOrderNo": {
                    "url": "/user/order/submitOrderNo/${userId}",
                    "version": "v1",
                    "versions": ["v1", "v2", "v3"]
                }
            }
        }
    },
    "ignoreUrls": {
        "/api/account4Client/login": true,
        "/api/qbii/news/getNewsList":true,
        "/api/qbii/news/getNewsDetail":true,
        "/api/qbii/v1/user/getUserLevel":true,

        "/api/item/config/wqTokenConfig": true
    },
    "redis": {
        "tokenKey": "nodeServer-token",
        "expiration": 2592000 
    },
    "cookie": {
        "signed": true,
        "maxAge": 2592000, 
        "secure": false, 
        "httpOnly": true
    },
    "SSO": false 
}