{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "生产编译",
            "program": "${workspaceRoot}/app/app.ts",
            "env": {
                "NODE_ENV": "production"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "program": "${workspaceRoot}/app/app.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序dist",
            "program": "${workspaceRoot}/dist/app.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
         {
            "type": "node",
            "request": "launch",
            "name": "测试",
            "program": "${workspaceRoot}/app/index.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "type": "node",
            "request": "attach",
            "name": "附加到进程",
            "address": "localhost",
            "port": 5858
        }
    ]
}